params {
    indir = "/input/workflow-inputs/Synthetic_Bwa"
}
process {
    container = 'friedricht/nf_wfbench:skip_mem_v11'
}
process {
    withName: 'task_fastq_reduce' {
        cpus = 1
        memory = '1.74GB'
    }
    withName: 'task_bwa_index' {
        cpus = 8
        memory = '21.79GB'
    }
    withName: 'task_bwa' {
        cpus = 2
        memory = '7.79GB'
    }
    withName: 'task_cat_bwa' {
        cpus = 3
        memory = '10.2GB'
    }
    withName: 'task_cat' {
        cpus = 1
        memory = '1.51GB'
    }
}
