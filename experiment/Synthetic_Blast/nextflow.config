params {
    indir = "/input/workflow-inputs/Synthetic_Blast"
}
process {
    container = 'friedricht/nf_wfbench:skip_mem_v11'
}
process {
    withName: 'task_split_fasta' {
        cpus = 1
        memory = '1.51GB'
    }
    withName: 'task_blastall' {
        cpus = 1
        memory = '21.79GB'
    }
    withName: 'task_cat_blast' {
        cpus = 1
        memory = '1.51GB'
    }
    withName: 'task_cat' {
        cpus = 1
        memory = '1.51GB'
    }
}
